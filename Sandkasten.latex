\begin{flushleft} \LARGE {Schlussfolgerung :}\end{flushleft}
\begin{flushleft} falls x $\not\le$ y dann ist x $>$ y \end{flushleft} 
\begin{flushleft} \LARGE{Frage :}gilt es zwei verschiedene Zahlen , die zueinander gleichartig sind ?\end{flushleft}
\begin{flushleft} \LARGE{Formal :} \hspace{1mm} $\exists$ $x$ , $y$ \hspace{1mm},\hspace{1mm} x $\le$ y $\wedge$ x $\ge$ y \hspace{1mm},\hspace{1mm} mit $x_L$ $\neq$ $y_L$ \end{flushleft}
\begin{flushleft} \underline{Beispiel}\LARGE{Beispiel :}\end{flushleft}
\begin{flushleft}$0$ $=$ $($ $\symbol{31}$ , $\symbol{31}$ $)$ ; y $=$ $($ ${$ $-1$ $}$ ,  $\symbol{31}$ $)$
\end{flushleft}














































































































