\begin{flushleft}
\LARGE \underline{{\color{red}\textbf{Frage:}}}









\vspace{5mm}\Large{kann es zwei Zahlen geben , die zueinander überhaupt  
keine Beziehung haben?}
\end{flushleft}
\vspace{5mm}\Large{Formal:}\hspace{2mm}$\exists$ $x$ , $y$ \hspace{2mm},\hspace{2mm}sodass x $\not\le$ y $\wedge$ y $\not\ge $ x\hspace{2mm}?

\vspace{5mm}\begin{flushleft}\LARGE{Vermutung:}\hspace{2mm}\Large{Es exestiert zwei Zahlen $x$,$y$\hspace{2mm},\hspace{2mm}die keine\hspace{10mm} Beziehung zueinander haben.}
\end{flushleft}

\vspace{5mm}\begin{flushleft}\LARGE{{\color{blue}(A)}}\hspace{1mm}:\hspace{1mm} \Large{Vermutung} $\Longleftrightarrow$ x $\not\le y$ $\wedge$ y$\not\ge x$ sind wahr.
\end{flushleft}

\vspace{5mm}\begin{flushleft}{Sei :} {$x$ \hspace{1mm},\hspace{1mm}$y$ \hspace{1mm}zwei schlechte Zahlen.}
\end{flushleft}


\vspace{5mm}\begin{flushleft}\underline{\LARGE{\textbf{Fallunterscheidung :}}}
\end{flushleft}
\begin{flushleft} x $\not\le$ y  $\Longleftrightarrow$ $X_L$  $\ge$ y  $\vee$  x $\ge$ $Y_R$ $\Longleftrightarrow$  $x_L$  $\ge$ y  $\vee$  x $\ge$ $y_R$  \end{flushleft}
\vspace{5mm}\begin{flushleft}
mit $x_L$\hspace{1mm}($y_R$)das grösste(das kleinste) Element von $X_L$ \hspace{1mm}($Y_R$).  
\end{flushleft}

\newpage
\vspace{5mm} \begin{flushleft}{1)} \end{flushleft}

\begin{flushleft}{wir nehmen an:}\hspace {1mm}{$x_L$ $\ge$ y }
\begin{flushleft}Falls $x_L$ $\le$ x ist \hspace{1mm},\hspace{1mm} gilt nach dem Transitivgesetz  y$\le$ x was unmöglich ist \hspace{1mm},\hspace{1mm} da nach  {{\color{blue}(A)}}\hspace{1mm} y $\not\le$ x \hspace{1mm},\hspace{1mm} also muss \begin{flushleft} $x_L$ $\not\le$ x \hspace{1mm}.\end{flushleft}
\end{flushleft}
\end{flushleft}

\vspace{5mm} \begin{flushleft}{2)} \end{flushleft}


\begin{flushleft}{wir nehmen an:}\hspace {1mm}{$y_R$ $\le$ x }
\begin{flushleft}Falls $y_R$ $\ge$ y ist \hspace{1mm},\hspace{1mm} gilt nach dem Transitivgesetz  y$\le$ x was {{\color{blue}(A)}}widerspricht \hspace{1mm},\hspace{1mm} also muss \begin{flushleft} $y_R$ $\not\ge$ y \hspace{1mm} sein .\end{flushleft}
\end{flushleft}
\end{flushleft}
\vspace{5mm} \begin{center}
\fbox {wir haben bis jetzt erreicht \hspace{1mm},\hspace{1mm} dass $y_R$ $\not\ge$ y $\wedge$ $x_L$ $\not\le$ x ist.}
 \end{center}
\vspace{3mm}

\begin{flushleft} $x_L$ $\not\le$ x besagt\hspace{2mm}:\hspace{2mm} Es gibt eine Zahl $x_{L_L}$ $\in$ $x_L$ mit \begin{flushleft} $x_{L_L}$ $\ge$ x oder eine Zahl $x_R$ in $X_R$ mit $x_L$ $\ge$ $x_R$ nach dem zweiten Regel. \end{flushleft}
\end{flushleft}
\vspace{5mm}
\begin{flushleft}  Formal \hspace{1mm}: \fbox {$x_L$ $\not\le$ x $\Longleftrightarrow$ $x_{L_L}$ $\ge$ x $\vee$ $x_L$ $\ge$ $x_R$} \LARGE {{\color{blue}(B)}}
\end{flushleft}



\vspace{5mm}
\begin{flushleft} $x_L$ $\ge$ $x_R$ verletzt aber den ersten Regel.
\end{flushleft}






