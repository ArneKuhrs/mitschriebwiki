  \hline
  $x_{L_L}$ $\ge$ x & $x_L$ $\ge$ $x_R$ & $x_L$ $\not\le$ x \\
  \hline \hline 
  \hspace{6mm}? & \hspace{6mm} 0 & ? \\
  \hline
\end{tabular}


\newpage


\begin{flushleft}
\vspace{5mm}Es bleibt zu zeigen \hspace{1mm},\hspace{1mm}dass $x_{L_L}$  $\ge$  x wahr oder falsch ist \hspace{1mm},\hspace{1mm} 
\vspace{5mm}um zu bestimmen ,dass $x_L$  $\not\le$  x  wahr oder falsch ist .
\end{flushleft}



\vspace{5mm}\begin{flushleft} \vspace{5mm}{Annahme {\color{blue}(C)}} 
  \vspace{5mm}\fbox{$x_{L_L}$ $\le$ $x_L$ $\Longleftrightarrow$
 $x_{L_L}$ ist früher erschaffen worden \hspace{1mm},\hspace{1mm}als $x_L$}  


\vspace{5mm}Nach {\color{blue}(B)} ist $x_{L_L}$ $\ge$ x \hspace{1mm},\hspace{1mm} also folgt nach
 dem \vspace{8mm}Transitivgesetz ,dass x $\le$ $x_L$ ist .


Nach dem 2. Regel




\begin{flushleft}
\vspace{8mm}\fbox{x $\le$ $x_L$ $\Longleftrightarrow$ \fbox{{\color{blue}(D)} \fbox{$x_L$ $\not\ge$ $x_L$}} $\wedge$ $x$ $\not\ge$ $x_{L_R}$} \hspace{2mm}  \LARGE {(E)}



