%Satz 7.4
\begin{satz}
$\log \in H(\MdC\_)$ und $\log'w= \frac1w \, \forall w \in \MdC\_$
\end{satz}

\begin{beweis}
Sei $w_0 \in \MdC\_$. Sei $(w_n)$ eine Folge in $\MdC\_$ mit: $w_n \not= w_0 \ \forall n \in \MdN$ und $w_n \rightarrow w_0, z_0 := \log w_0, z_n:=\log w_n. 7.3 \Rightarrow z_n \rightarrow z_0.$ Dann:\\
\[
\frac{\log w_n - \log w_0}{w_n - w_0} = \frac{z_n-z_0}{e^{z_n}-e^{z_0}} = \left(\frac{e^{z_n}-e^{z_0}}{z_n -z_0}\right)^{-1} \rightarrow \frac1{e^{z_0}} = \frac1{w_0}
\]
D.h. $\log$ ist in $w_0$ komplex db und $\log'w_0 = \frac1{w_0}$
\end{beweis}

\emph{Bezeichnung:}
\[
D := \{ z \in \MdC : |z| < 1 \} = U_1(0)
\]

\emph{Beachte:}\\
F"ur $z \in D$ ist $1-z \in \MdC\_$

%Satz 7.5
\begin{satz}
\[
\log (1+z) = \sum_{n=1}^{\infty} (-1)^{n+1} \frac{z^n}{n} \ \forall z \in D
\]
\end{satz}

$\MdD$
