\hh{VERTEX-COVER}
\hhh{Eingabe} ungerichteter Graph $G=(V, E), k\in \mathbb{N}$ \\
\hhh{Ausgabe} $\exists V'\subseteq V:|V'|=k \wedge \forall\{u, v\}\in E: u\in V'\vee v\in V'$ \\
\hhh{NP-hart via} CLIQUE \\

\hh{COLORING}
\hhh{Eingabe} ungerichter Graph $G=(V, E), k\in \mathbb{N}$ \\
\hhh{Ausgabe} Gibt es eine Knotenfärbung $c: V\to{1, \ldots, k}$ mit $\forall\{u, v\}\in E: c(u)\ne c(v).$ \\
\hhh{NP-hart via} 3SAT \\

\hh{SUBSET-SUM}
\hhh{Eingabe} $n$ Gegenstände mit Gewicht $w_i \in\mathbb{N}, W\in \mathbb{N}$ \\
\hhh{Ausgabe} $\exists M \subseteq \{1, \ldots, n\}: \Sigma_{i \in M} w_i = W$? \\
\hhh{NP-hart via} 3SAT \\

\hh{PARTITION}
\hhh{Eingabe} $n$ Gegenstände mit Gewicht $w_i \in\mathbb{N}$ \\
\hhh{Ausgabe} $\exists M \subseteq \{1, \ldots, n\}: \Sigma_{i\in M} w_i = \Sigma_{i\not\in M}w_i$? \\
\hhh{NP-hart via} SUBSET-SUM \\

\hh{ILP}
\hhh{Eingabe} Vektor $x=(x_1, \ldots, x_n)$, Bedingungen $a\cdot xRb$ mit $R\in\{\le, \ge, =\}, b\in \mathbb{Z}, a\in\mathbb{Z}^n$\\
\hhh{Ausgabe} $\exists$ Belegung für $x$, damit alle Bedingungen wahr sind?\\
\hhh{NP-hart via} SUBSET-SUM\\

\hh{BIN-PACKING}
\hhh{Eingabe} $n$ Gegenstände mit Größe $w_i \in\mathbb{N}$, Behältergröße $b\in\mathbb{N}$, Anzahl Behälter $k$\\
\hhh{Ausgabe} $\exists f:1\ldots n\to 1\ldots k:\forall j\in 1\ldots k:\Sigma\{w_i:f(i)=j\}\le b$\\
\hhh{NP-hart via} PARTION\\

\h{Grammatiken}
Sei $G=(V, \Sigma, P, S)$, $\forall l\to r\in P:$\\
\hh{Definition CH-0}
beliebig\\
\hh{Definition CH-1}
$|l|\le |r|$. Sonderregel für $\varepsilon$-Produktion nur bei $S$\\
\hh{Definition CH-2}
Typ 1 und $l\in V$\\
\hhh{Pumpinglemma, kontextfrei}
$L$ kontextfrei $\Rightarrow\exists n\in\mathbb{N}:\forall z\in L: |z|> n\Rightarrow \exists u, v, w, x, y:z=uvwxy\wedge|vx|\ge 1\wedge|vwx|\le n \wedge \forall i\in \mathbb{N}_0:uv^iwx^iy\in L$\\
\hhh{Chomsky-Normalform}
falls gilt: $P\subseteq (V\times\Sigma)\cup( V\times VV)$\\
\begin{enumerate}
\setlength{\itemindent}{-5ex}
\setlength{\itemsep}{-1ex}
\item Terminale in eigene Regeln verbannen.
\item Seiten die länger sind als zwei Nicht-Terminal aufsplitten
\item $\varepsilon$-Produktionen entfernen
\item Kettenproduktionen entfernen
\end{enumerate}

\hh{Definition CH-3}
Typ 2 und $r\in\Sigma\cup\Sigma V$\\
\hhh{Pumpinglemma, regulär}
$L$ regulär $\Rightarrow\exists n\in\mathbb{N}:\forall w\in L:|w|> n\Rightarrow\exists w, v, x:w=uvx\wedge|v|\ge 1\wedge|uv|\le n\wedge\forall i\in\mathbb{N}_0:uv^ix\in L$\\
\hhh{Nerode-Relation}
$R_L:=\{(x, y)\Sigma^*\times\Sigma^*:\forall z\in\Sigma^*:xz\in L\Leftrightarrow yz\in L\}$\\Sprache $L$ regulär $\Leftrightarrow$ index$(R_L)\ne\infty$\\
\hhh{\FM{Syntax reguläre Ausdrücke}}
$(\emptyset\cup\varepsilon)^*abc^+$\\
